{
  "name": "sheety-writer",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://fmilitao.github.io/sheety-writer/build",
  "dependencies": {
    "@material-ui/core": "^4.3.0",
    "@material-ui/icons": "^4.2.1",
    "@types/crypto-js": "^3.1.43",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/react-select": "^3.0.0",
    "ajv": "^6.10.2",
    "crypto-js": "^3.1.9-1",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "react-select": "^3.0.4",
    "typescript": "3.5.3"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "scripts": {
    "encrypt:build": "tsc --out build/encrypt.js script/encrypt.ts",
    "encrypt:file": "node build/encrypt.js",
    "decrypt:build": "tsc --out build/decrypt.js script/decrypt.ts",
    "decrypt:file": "node build/decrypt.js",
    "lint": "prettier --check 'src/**/*'",
    "lint:fix": "prettier --check 'src/**/*' --write",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "echo 'No tests! :('",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^2.0.1"
  }
}
